# Configuración de desarrollo local
NODE_ENV=development
DB_TYPE=dynamodb
JWT_SECRET=desarrollo-secreto-jwt-cambiar-en-produccion
PORT=3000

# Para desarrollo con DynamoDB local (si usas LocalStack)
# AWS_REGION=us-east-1
# AWS_ACCESS_KEY_ID=localstack
# AWS_SECRET_ACCESS_KEY=localstack
# DYNAMODB_ENDPOINT=http://localhost:4566

# Para desarrollo con DynamoDB real
AWS_REGION=us-east-1
# AWS_ACCESS_KEY_ID=your_access_key
# AWS_SECRET_ACCESS_KEY=your_secret_key

# Tabla DynamoDB (se configura automáticamente con Serverless)
DYNAMODB_TABLE=sistema-espacios-hospitalarios-dev-table

# SQS Queue (se configura automáticamente con Serverless)
QUEUE_URL=sistema-espacios-hospitalarios-dev-queue
