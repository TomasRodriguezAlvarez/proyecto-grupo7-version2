# Archivo .env.example
# Copia este archivo como .env y completa con tus valores

# Credenciales AWS - Necesarias para despliegue y ejecución local
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_REGION=us-east-1

# Secretos de aplicación
JWT_SECRET=cambia-esto-por-una-clave-secreta-segura

# Configuración de entorno
STAGE=dev

# Variables para DynamoDB local (solo para desarrollo)
DYNAMODB_LOCAL=true
DYNAMODB_ENDPOINT=http://localhost:8000

# Variables para testing
TEST_USER_EMAIL=
TEST_USER_PASSWORD=

# Variables para notificaciones 
ADMIN_EMAIL=admin@example.com
ALERT_PHONE=+1234567890

# Variables para frontend
NEXT_PUBLIC_API_URL=https://api.example.com
NEXT_PUBLIC_WS_URL=wss://ws.example.com